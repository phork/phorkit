{
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "importOrder": [
    "^lib(.*)$",
    "^__mocks__(.*)$",
    "^config(.*)$",
    "^context(.*)$",
    "^hooks(.*)$",
    "^utils(.*)$",
    "^icons(.*)$",
    "^components(.*)$",
    "^compositions(.*)$",
    "^stories(.*)$",
    "^helpers(.*)$",
    "^styles(.*)$",
    "^(../)+types(.*)$",
    "^(../)+config(.*)$",
    "^(../)+context(.*)$",
    "^(../)+hooks(.*)$",
    "^(../)+utils(.*)$",
    "^(../)+icons(.*)$",
    "^(../)+components(.*)$",
    "^(../)+compositions(.*)$",
    "^(../)+stories(.*)$",
    "^(../)+helpers(.*)$",
    "^(../)+styles(.*)$",
    "^(../)+",
    "^./config(.*)$",
    "^./context(.*)$",
    "^./hooks(.*)$",
    "^./utils(.*)$",
    "^./icons(.*)$",
    "^./components(.*)$",
    "^./compositions(.*)$",
    "^./stories(.*)$",
    "^./helpers(.*)$",
    "^./styles(.*)$",
    "^./(.*)$"
  ],
  "importOrderSeparation": false,
  "experimentalBabelParserPluginsList": ["jsx", "typescript"]
}
